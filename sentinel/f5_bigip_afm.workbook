    {
      "type": 1,
      "content": {
        "json": "---\r\n## Network Firewall Logs\r\n---"
      },
      "name": "text - 13"
    },
    {
      "type": 9,
      "content": {
        "version": "KqlParameterItem/1.0",
        "parameters": [
          {
            "id": "513a762e-8c07-4ebc-b792-15756c834316",
            "version": "KqlParameterItem/1.0",
            "name": "parmUseBrush",
            "label": "Use Brush Time",
            "type": 10,
            "description": "Using this allows you to filter the search to any time period selected (brushed) on the 'Policy Matches over Time Period' chart.",
            "isRequired": true,
            "typeSettings": {
              "additionalResourceOptions": [],
              "showDefault": false
            },
            "jsonData": " [{ \"value\": \"1\", \"label\": \"Yes\" }, { \"value\": \"0\", \"label\": \"No\", \"selected\":true }]"
          },
          {
            "id": "57397577-b381-477d-8a6f-70545a553e14",
            "version": "KqlParameterItem/1.0",
            "name": "parmFilterAction",
            "label": "Filter (Action)",
            "type": 2,
            "query": "\r\nF5Telemetry_AFM_CL\r\n| where context_type_s == 'Global' \r\n| distinct action_s\r\n",
            "typeSettings": {
              "additionalResourceOptions": [
                "value::1"
              ],
              "showDefault": false
            },
            "defaultValue": "value::1",
            "queryType": 0,
            "resourceType": "microsoft.operationalinsights/workspaces",
            "value": "Drop"
          },
          {
            "id": "bf5490e6-d1d9-4ac7-bc1c-7f4535133f2c",
            "version": "KqlParameterItem/1.0",
            "name": "parmOrigIP",
            "label": "Originating IP",
            "type": 2,
            "multiSelect": true,
            "quote": "'",
            "delimiter": ",",
            "query": "\r\nF5Telemetry_AFM_CL\r\n| where context_type_s == 'Global' \r\n| distinct source_ip_s\r\n",
            "typeSettings": {
              "additionalResourceOptions": [],
              "showDefault": false
            },
            "queryType": 0,
            "resourceType": "microsoft.operationalinsights/workspaces",
            "value": [
              "172.16.128.148"
            ]
          }
        ],
        "style": "pills",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "name": "parameters - 1 - Copy"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "F5Telemetry_AFM_CL \r\n| where action_s == '{parmFilterAction}' or '{parmFilterAction}' == ''\r\n| where iff(isnotempty('{q15brush}') and {parmUseBrush} == 1 ,TimeGenerated {q15brush},TimeGenerated {parmTimeRange})\r\n| where isempty(insp_id_s) and drop_reason_s != 'Connection Flow Miss'\r\n| where isempty('{parmOrigIP:label}') or source_ip_s in (split('{parmOrigIP:label}', \",\"))\r\n| extend dest_ip_mod = strcat(dest_ip_s,\":\",dest_port_s)\r\n| join kind=leftouter (F5Telemetry_virtualServers_CL\r\n   | distinct destination_s, name_s ) on $left.dest_ip_mod == $right.destination_s\r\n| project TimeGenerated, tostring(split(acl_policy_name_s, '/')[-1]), tostring(split(acl_rule_name_s, ':')[-1]), action_s, source_ip_s, source_port_s, dest_ip_s, Port = strcat(tolower(ip_protocol_s), \"/\", dest_port_s), Virtual_Server = tostring(split(name_s, '/')[-1]) ,hostname_s\r\n| sort by toint(TimeGenerated) desc",
        "size": 0,
        "showAnalytics": true,
        "showRefreshButton": true,
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "gridSettings": {
          "formatters": [
            {
              "columnMatch": "action_s",
              "formatter": 18,
              "formatOptions": {
                "thresholdsOptions": "colors",
                "thresholdsGrid": [
                  {
                    "operator": "==",
                    "thresholdValue": "Drop",
                    "representation": "redBright",
                    "text": "{0}{1}"
                  },
                  {
                    "operator": "==",
                    "thresholdValue": "Accept",
                    "representation": "green",
                    "text": "{0}{1}"
                  },
                  {
                    "operator": "Default",
                    "thresholdValue": null,
                    "representation": "lightBlue",
                    "text": "{0}{1}"
                  }
                ]
              }
            }
          ],
          "filter": true
        }
      },
      "name": "query - 13 - Copy"
    },
    {
      "type": 1,
      "content": {
        "json": "<br>\r\n\r\n---\r\n## F5 BIG-IP Protocol Inspection (IDS/IPS) Insights\r\n---"
      },
      "name": "text - 9"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "F5Telemetry_AFM_CL \r\n| where hostname_s == '{HostName}' or '{HostName}' == \"All\"\r\n| where isnotempty(insp_id_s)\r\n| summarize Count = count() by ['IPS Action'] = action_s, risk_s, insp_name_s\r\n| order by ['IPS Action'] asc",
